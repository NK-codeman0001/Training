CREATE SEQUENCE public.person_pid_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.person_pid_seq OWNER TO postgres;

--
-- Name: person_pid_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public.person_pid_seq OWNED BY public.person.pid;


--
-- Name: person_with_car_view; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.person_with_car_view AS
 SELECT person.car_id,
    person.pid,
    person.name,
    person.dob,
    person.yop,
    person.gender,
    person.age,
    person.country,
    car.model_name,
    car.company,
    car.price
   FROM (public.person
     JOIN public.car USING (car_id));


ALTER TABLE public.person_with_car_view OWNER TO postgres;

--
-- Name: person_without_car_view; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.person_without_car_view AS
 SELECT person.name,
    person.dob,
    person.age,
    person.yop,
    person.gender,
    person.country
   FROM (public.person
     LEFT JOIN public.car ON ((person.car_id = car.car_id)))
  WHERE (person.car_id IS NULL);


ALTER TABLE public.person_without_car_view OWNER TO postgres;

--
-- Name: test; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.test (
    id bigint NOT NULL,
    "full name" character varying(60) NOT NULL,
    dob date,
    scheme text[],
    saving integer[],
    student public.student,
    CONSTRAINT age_constraint CHECK ((age(now(), (dob)::timestamp with time zone) > '18 years'::interval))
);


ALTER TABLE public.test OWNER TO postgres;

--
-- Name: test_id_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE public.test_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.test_id_seq OWNER TO postgres;

--
-- Name: test_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE public.test_id_seq OWNED BY public.test.id;


--
-- Name: car car_id; Type: DEFAULT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.car ALTER COLUMN car_id SET DEFAULT nextval('public.car_car_id_seq'::regclass);


--
-- Name: person pid; Type: DEFAULT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.person ALTER COLUMN pid SET DEFAULT nextval('public.person_pid_seq'::regclass);


--
-- Name: test id; Type: DEFAULT; Schema: public; Owner: postgres
--

ALTER TABLE ONLY public.test ALTER COLUMN id SET DEFAULT nextval('public.test_id_seq'::regclass);


